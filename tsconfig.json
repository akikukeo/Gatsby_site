{
    "compilerOptions": {
        "jsx": "react",
        "target": "ES2020",
        "allowJs": true,
        "module": "commonjs",
        "strict": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "outDir": "./dist"
    },
    "include": ["Gatsby_site/src/**/*.ts", "Gatsby_site/src/**/*.js", "Gatsby_site/src/pages/404.tsx", "Gatsby_site/gatsby-ssr.js"],
    "exclude": [
        "node_modules",
        "./Gatsby_site/node_modules",
        "dist",
        "**/node_modules",
        "**/dist",
        "**/build",
        "**/*.min.js",
        "**/coverage",
        "build"
    ]
}
